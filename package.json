  "devDependencies": {
    "@dada78641/eslint-config": "^1.0.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.3.0",
    "eslint": "^9.34.0",
    "jiti": "^2.5.1",
    "tsdown": "^0.20.0-beta.3",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "lodash-es": "^4.17.22",
    "obs-websocket-js": "^5.0.7"
